@model IEnumerable<Deposit>


<div class="col-md-6 ">
    <div class="dash_blog">
        <div class="dash_blog_inner">
            <div class="dash_head">
                <h3><span><i class="fa fa-calendar"></i>Group A @DateTime.Now</span></h3>
            </div>
            <div class="list_cont">
                <p>Daily Group Updates From 5 Days Ago To Date</p>
            </div>
            <div class="task_list_main">
                <ul class="task_list">
                    @if(Model !=null){
                    @foreach (var member2 in Model)
                    {
                        <li><a href="#">@member2.Invoices.Select(x => x.Account.ApplicationUser.FirstName).FirstOrDefault() 
                                @member2.Invoices.Select(x => x.Account.ApplicationUser.LastName).FirstOrDefault(),
                                Has Deposited R @member2.DepositAmount.ToString("0.00") To Group
                            </a><br><strong>
                                @member2.DepositDate
                            </strong>
                        </li>
                    }}
                </ul>
            </div>
        </div>
    </div>
</div>
